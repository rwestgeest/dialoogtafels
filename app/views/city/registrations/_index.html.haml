%tr
  %td.input_column_eq_left
    - if (available_locations.empty?)
      %h3 Geen locaties beschikbaar
    - else
      %h3 Beschikbare locaties
      #available-locations
        = render partial: 'location', collection: available_locations
  %td.input_column_eq_right
    #registered_conversations
      = render :partial => 'registered_conversations'

