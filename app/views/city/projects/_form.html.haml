= form_for [:city, @project] do |f|
  -if @project.errors.any?
    #error_explanation
      %h2= "#{pluralize(@project.errors.count, "error")} prohibited this city_project from being saved:"
      %ul
        - @project.errors.full_messages.each do |msg|
          %li= msg

  %p
    .label= f.label :name
    .input= f.text_field :name
  .actions
    = f.submit 
